public class Number40
{
	public static void main(String[] args)
	{
		long a = 1;
		int counter = 1;
		long product = 1;
		while (counter <= 1000000)
		{
			String a1 = Long.toString(a);
			int length = a1.length();
			System.out.print(a1);
			if (counter == 1)
			{
				product = (int) (product * a);
			}
			if (counter == 10)
			{
				product = (int) (product * a);
			}
			if (counter == 100)
			{
				product = (int) (product * a);
			}
			if (counter == 1000)
			{
				product = (int) (product * a);
			}
			if (counter == 10000)
			{
				product = (int) (product * a);
			}
			if (counter == 100000)
			{
				product = (int) (product * a);
			}
			if (counter == 1000000)
			{
				product = (int) (product * a);
			}
			a++;
			counter += length;
		}
		System.out.println("");
		System.out.println("The answer to Project Euler #40 is " + product);

	}

}
