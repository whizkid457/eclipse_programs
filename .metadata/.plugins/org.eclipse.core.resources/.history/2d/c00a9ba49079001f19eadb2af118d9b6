public class Number21 
{
	public static void main(String[] args) 
	{
		int number = 3;
		int divisor = 2;
		int sum = 1;
		while (number < 1000000000)
		{
			while (number >= divisor)
			{
				if (number == divisor)
				{
					number++;
					divisor = 2;
					System.out.print("The sum of " + number + "'s divisors is ");
					break;					
				}
				if (number % divisor == 0)
				{
					sum = sum + divisor;
					divisor++;
				}
				if (number % divisor != 0)
				{
					divisor++;
				}
			}
			System.out.println(sum);
		}
		
		
		
		
		
		
		

	}

}
