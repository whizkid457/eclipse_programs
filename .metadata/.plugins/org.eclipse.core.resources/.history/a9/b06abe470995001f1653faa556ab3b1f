public class Number40
{
	public static void main(String[] args)
	{
		int a = 1;
		int counter = 1;
		int product = 1;
		while (counter <= 1000000)
		{
			String a1 = Integer.toString(a);
			int length = a1.length();
			System.out.print(a1);
			if (counter == 1)
			{
				product = product * a;
			}
			a++;
			counter += length;
		}

	}

}
