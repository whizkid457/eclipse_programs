public class Number9
{
	public static void main(String[] args)
	{
		for (int a = 1, b = 1, c = 1; c < 1000; a++)
		{
			if (a == 1000)
			{	
				a = 1;
				b++;
			}
			if (b == 1000)
			{
				b = 1;
				c++;
			}
			if (a + b + c == 1000) System.out.println(a + " " + b + " " + c);
		}
	}

}
