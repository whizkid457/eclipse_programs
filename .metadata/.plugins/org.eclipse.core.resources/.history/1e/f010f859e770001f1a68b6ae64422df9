
public class Loop {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		int targetBalance = 0;
		int balance = 0;
		// TODO Auto-generated method stub
		while (balance < targetBalance);
		{
			int years = 0;
			years++;
			int rate = 0;
			double interest = balance * rate / 100;
			balance = (int) (balance + interest);
		}
	}

}
