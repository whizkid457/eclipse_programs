public class Number10 
{
	public static void main(String[] args) 
	{
		int divisor = 2;
		int number = 3;
		long sum = 0;
		while (number <= 2000000)
		{
			while (number % divisor != 0)
			{	
				if (number == divisor + 1)
				{
					System.out.println(number);
					sum = sum + number;
				}
				divisor++;
			}
			divisor = 2;
			number++;
		}
		System.out.println("The answer to Project Euler #10 is " + sum);
	}
}
