public class Number21 
{
	public static void main(String[] args) 
	{
		int number = 3;
		int divisor = 2;
		int sum = 0;
		while (number <= 100)
		{
			while (number >= divisor)
			{
				if (number == divisor)
				{
					number++;
					divisor = 2;
					break;
				}
				if (number % divisor == 0)
				{
					sum = sum + divisor;
					divisor++;
				}
				if (number % divisor == 1)
				{
					divisor++;
				}
			}
			System.out.println(sum);
		}
		
		
		
		
		
		
		

	}

}
